# proxy_pass http://${container_name}:${container_port};
proxy_pass http://127.0.0.1:8001;
proxy_set_header Host $host:$proxy_port;
proxy_set_header X-Real-IP $remote_addr;
proxy_set_header X-Forwarded-For $remote_addr;
